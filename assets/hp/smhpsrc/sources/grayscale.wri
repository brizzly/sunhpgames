GOSUBL AF
A=0.W
{
 D0=(5) $00128
 A=DAT0 6
 ?A#0 W UP
}
D1=(5) $8011B A=DAT1.A
LC 00880 A+C.A D1=A
GOSUBL AF
A=0.W
{
 D0=(5) $00128
 A=DAT0 6
 ?A#0 W UP
}
